{
    "name": "mymocms/mymocms",
    "description": "Mymo is a Content Management System (CMS) developed based on Laravel Framework and web platform whose sole purpose is to make your development workflow simple again.",
    "keywords": [
      "laravel",
      "laravel cms",
      "mymo cms",
      "laravel cms like wordpress",
      "cms like wordpress"
    ],
    "license": "MIT",
    "homepage": "https://github.com/mymocms/mymocms",
    "authors": [
        {
          "name": "The Anh Dang",
          "email": "dangtheanh16@gmail.com",
          "role": "Developer"
        }
    ],
    "support": {
      "email": "dangtheanh16@gmail.com",
      "docs": "https://github.com/mymocms/mymocms"
    },
    "require": {
        "php": ">=7.2.5",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.5",
        "hassankhan/config": "^2.2",
        "intervention/image": "^2.5",
        "laravelium/sitemap": "6.*",
        "pion/laravel-chunk-upload": "^1.4",
        "sokil/php-vast": "^2.0",
        "spatie/image-optimizer": "^1.4",
        "spatie/laravel-permission": "^4.0",
        "symfony/yaml": "^5.2",
        "tadcms/hooks": "^1.0",
        "tymon/jwt-auth": "^1.0"
    },
    "require-dev": {
      "laravel/laravel": "^6.0 | ^7.0 | ^8.0",
      "phpunit/phpunit": "6.0 | 7.0 | 7.5 | 8.4 | ^8.5"
    },
    "autoload": {
        "psr-4": {
          "Mymo\\Tests\\": "tests/",
          "Mymo\\": "src/"
        },
        "files": [
            "src/Core/Helpers/helpers.php",
            "src/Plugin/helpers.php",
            "src/Installer/Helpers/functions.php",
            "src/Theme/Helpers/helpers.php"
        ]
    },
    "extra": {
      "laravel": {
        "providers": [
          "Mymo\\Core\\Providers\\MymoCoreServiceProvider",
          "Mymo\\Plugin\\PluginServiceProvider"
        ]
      }
    }
}