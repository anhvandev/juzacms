{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "arcanedev/log-viewer": "~4.7.0",
        "davejamesmiller/laravel-breadcrumbs": "5.x",
        "dilab/resumable.php": "^0.1.4",
        "esemve/hook": "^0.0.6",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.5",
        "ip2location/ip2location-laravel": "^1.0",
        "laravel/framework": "5.8.*",
        "laravel/tinker": "^1.0",
        "laravelium/sitemap": "3.1.*",
        "nwidart/laravel-modules": "^6.2",
        "rachidlaasri/laravel-installer": "^4.1",
        "spatie/laravel-permission": "^3.10",
        "unisharp/laravel-filemanager": "^2.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.7",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
          "app/Helpers/functions.php",
          "app/Helpers/VideoStream.php"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
