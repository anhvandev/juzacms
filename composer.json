{
    "name": "mymocms/mymocms",
    "type": "project",
    "description": "MyMo is a powerful, flexible and User friendly movie & Video Steaming CMS Pro with advance video contents management system. Itâ€™s easy to use & install. It has been created to provide unique experience to movie lover & movie site owner.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "authors": [
      {
        "name": "The Anh Dang",
        "email": "dangtheanh16@gmail.com"
      }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.2.5",
        "aminyazdanpanah/php-ffmpeg-video-streaming": "^1.2",
        "arcanedev/log-viewer": "5.*",
        "diglactic/laravel-breadcrumbs": "6.x",
        "dilab/resumable.php": "^0.1.4",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.5",
        "ip2location/ip2location-laravel": "^1.0",
        "laravel/framework": "6.*",
        "laravel/tinker": "^1.0",
        "laravelium/sitemap": "6.*",
        "nwidart/laravel-modules": "^8.2",
        "pion/laravel-chunk-upload": "^1.4",
        "sokil/php-vast": "^2.0",
        "spatie/laravel-permission": "^3.10",
        "unisharp/laravel-filemanager": "^2.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.7",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
          "app/Helpers/functions.php",
          "app/Helpers/simple_html_dom.php",
          "app/Helpers/VideoStream.php",
          "app/Helpers/UrlVideoStream.php",
          "app/Helpers/Stream3sApi.php",
          "app/Helpers/TmdbConfiguration.php",
          "app/Helpers/TmdbApi.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
